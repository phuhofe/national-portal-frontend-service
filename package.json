    "build:test": "sh ./build.sh test",
    "build:pre-production": "sh ./build.sh pre-production",
    "build:production": "sh ./build.sh production",
    "build:nova": "sh ./build.sh nova",
    "link-ui": "npm link @adstate_as/flora",
    "unlink-ui": "npm unlink && npm install",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser firefox"
